{
  "$schema": "http://json.schemastore.org/tslint",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "@n4bb12/config-tslint"
  ],
  "rules": {
    "arrow-parens": false,
    "jsx-boolean-value": [
      true,
      "never"
    ],
    "member-access": [
      true,
      "no-public"
    ]
  }
}
