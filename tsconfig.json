{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "outDir": "dist",
    "pretty": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "types": ["react"]
  },
  "include": ["src"]
}
